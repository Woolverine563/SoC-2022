   ],
   "source": [
    "counts = svsim.run(assemble(qc, shots=10000)).result().get_counts()\n",
    "x = counts['0']/10000\n",
    "print(\"Calculated inner product = \", np.sqrt(2*x - 1))\n",
    "plot_histogram(counts)"
   ]
