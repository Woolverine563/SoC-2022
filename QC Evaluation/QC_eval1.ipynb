   ],
   "source": [
    "qc = QuantumCircuit(2, 2)\n",
    "qc.initialize(state_1, 0)\n",
    "qc.initialize(state_2, 1)\n",
    "qc.barrier()\n",
    "qc.cx(0, 1)\n",
    "qc.h(0)\n",
    "qc.barrier()\n",
    "qc.measure([0, 1], [0, 1])\n",
    "qc.draw('mpl')"
   ]
  },
