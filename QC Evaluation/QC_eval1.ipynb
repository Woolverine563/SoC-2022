     },
     "execution_count": 9,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "qc = QuantumCircuit(3, 1)\n",
    "qc.initialize([1, 0], 0)\n",
    "qc.initialize(state_1, 1)\n",
    "qc.initialize(state_2, 2)\n",
    "qc.h(0)\n",
    "qc.barrier()\n",
    "qc.ccx(0, 1, 2)\n",
    "qc.ccx(0, 2, 1)\n",
    "qc.ccx(0, 1, 2)\n",
    "qc.barrier()\n",
    "qc.h(0)\n",
    "qc.measure(0, 0)\n",
    "qc.draw('mpl')"
   ]
  },
  {
