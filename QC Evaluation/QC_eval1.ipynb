     "execution_count": 12,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "counts = svsim.run(assemble(qc, shots=10000)).result().get_counts()\n",
    "states = ['00', '01', '10', '11']\n",
    "for i in states:\n",
    "    if i not in counts.keys():\n",
    "        counts[i] = 0\n",
    "x = (counts['00'] + counts['01'] + counts['10'] - counts['11'])/10000\n",
    "print(\"Calculated inner product = \", sqrt(x))\n",
    "plot_histogram(counts)"
   ]
